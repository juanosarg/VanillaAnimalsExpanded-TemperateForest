<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  
  <!-- Goose Eggs -->
  <ThingDef ParentName="EggUnfertBase">
    <defName>AEXP_EggWildGooseUnfertilized</defName>
    <label>wild goose egg (unfert.)</label>
    <description>An unfertilized wild goose egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <color>(228,185,135)</color>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>9.0</MarketValue>
    </statBases>
  </ThingDef>
  <ThingDef ParentName="EggFertBase">
    <defName>AEXP_EggWildGooseFertilized</defName>
    <label>wild goose egg (fert.)</label>
    <description>A fertilized wild goose egg. If all goes well, it should hatch into a baby wild gosling. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(235,235,235)</color>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>11.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6.5</hatcherDaystoHatch>
        <hatcherPawn>AEXP_WildGoose</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
</Defs>
