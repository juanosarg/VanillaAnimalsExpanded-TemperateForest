<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Advanced Biomes (Continued)</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>






			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_RedPanda"]/race/wildBiomes</xpath>
				<value>
					<PoisonForest>0.6</PoisonForest>
					<Wetland>0.05</Wetland>					
				</value>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Badger"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.2</PoisonForest>
					<Wetland>0.05</Wetland>	
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_WildGoose"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.5</PoisonForest>
					<Wetland>0.1</Wetland>	
				</value>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Beaver"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.2</PoisonForest>
					<Wetland>0.04</Wetland>	
				</value>
			</li>
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Hedgehog"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>1</PoisonForest>
					<Wetland>0.2</Wetland>	
				</value>
			</li>

			

			

		</operations>
		</match>
	</Operation>


</Patch>

